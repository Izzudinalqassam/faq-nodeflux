{
  "name": "faq-nodeflux",
  "version": "1.0.0",
  "description": "Nodeflux FAQ Center - Fullstack application",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && python app.py",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd backend && pip install -r requirements.txt && cd ../frontend && npm install",
    "build": "cd frontend && npm run build"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  },
  "keywords": ["nodeflux", "faq", "react", "flask", "fullstack"],
  "author": "Nodeflux Team",
  "license": "MIT"
}